{
  "name": "bookapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"node index.js\" \"cd client && react-scripts start\"",
    "start": "node index.js",
    "build:ui": "rm -rf build && cd client && npm run build && cp -r build ../",
    "deploy": "git push heroku main",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push && npm run deploy",
    "logs:prod": "heroku logs --tail",
    "lint": "eslint ."
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.6",
    "mongoose-unique-validator": "^3.0.0",
    "nodemon": "^2.0.15"
  },
  "devDependencies": {
    "supertest": "^6.2.1"
  }


}
